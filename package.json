    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-qr-code": "^2.0.12",
    "recharts": "^2.9.0",
    "svix": "^1.12.0",
    "swr": "^2.2.4",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "dotenv-cli": "^7.3.0",
    "encoding": "^0.1.13",
    "husky": "^8.0.0",
    "prisma": "^5.3.1",
    "vitest": "^0.34.5"
  }
