{
  "name": "site-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "yarn db:gen",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "test": "vitest run",
    "db:gen": "prisma generate",
    "db:push": "dotenv -e .env -- prisma db push",
    "db:migrate": "dotenv -e .env -- prisma migrate dev --name",
    "db:ui": "dotenv -e .env -- npx prisma studio"
  },
  "dependencies": {
    "@clerk/localizations": "^1.26.1",
    "@clerk/nextjs": "^4.24.0",
    "@hapi/boom": "^10.0.1",
    "@prisma/client": "^5.3.1",
    "@scure/bip39": "^1.2.1",
    "@signumjs/core": "^1.0.0-rc.75",
    "@signumjs/crypto": "^1.0.0-rc.75",
    "@tailwindcss/forms": "^0.5.6",
    "@types/node": "20.6.3",
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.8",
    "@veridibloc/smart-contracts": "file:veridibloc-smart-contracts-v0.0.3.tgz",
    "autoprefixer": "10.4.16",
    "date-fns": "^2.30.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.5.2",
    "lodash.debounce": "^4.0.8",
    "next": "13.5.3",
    "next-intl": "3.0.0-rc.2",
